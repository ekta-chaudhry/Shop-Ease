<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length <= 0) { %>
                <h1>Nothing there!</h1>
            <% }else { %>
                <ul>
                    <% orders.forEach(order => {%>
                        <li> 
                            <h1>#<%= order._id %></h1>
                            <ul>
                                <% order.items.forEach(item => { %>
                                    <li>
                                        <%=item.title %> ( <%= item.quantity %>)
                                    </li>
                                <% }); %>
                            </ul>
                        </li>
                    <% }); %>
                </ul>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>